    "firebase-admin": "^7.0.0",
    "firebase-functions": "^2.3.1",
    "rlp": "^2.2.3",
    "twilio": "^3.23.2",
    "web3": "1.0.0-beta.37",
    "web3-core-helpers": "1.0.0-beta.37"
  },
  "devDependencies": {
    "@types/debug": "^4.1.4",
    "@types/twilio": "^0.0.10",
    "@types/web3": "^1.0.19",
    "@types/yargs": "^13.0.2",
    "cross-env": "5.2.0",
    "firebase-bolt": "^0.8.4",
    "firebase-tools": "^7.3.0",
    "rimraf": "2.6.3",
    "yargs": "14.0.0"
