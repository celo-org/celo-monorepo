{
  "compileOnSave": false,
  "compilerOptions": {
    "lib": ["es2015", "es2016"],
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "composite": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "strict": true,
    "declaration": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  }
}
