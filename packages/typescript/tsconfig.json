{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": false,
    "baseUrl": ".",
    "importHelpers": true,
    "jsx": "react",
    "lib": ["es7", "esnext", "dom"],
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "target": "es3",
    "strict": true
  },
  "exclude": ["node_modules"]
}
