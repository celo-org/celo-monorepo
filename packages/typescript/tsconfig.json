{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": false,
    "baseUrl": ".",
    "importHelpers": true,
    "lib": ["es7", "esnext"],
    "moduleResolution": "node16",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "target": "es6",
    "strict": true
  },
  "exclude": ["node_modules"]
}
