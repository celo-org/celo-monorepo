{
  "extends": "@tsconfig/recommended/tsconfig.json",
  "compilerOptions": {
    "outDir": "lib",
    "rootDir": "src",
    "baseUrl": ".",
    "strict": false,
    "lib": ["es7", "es2017", "es2020"],
    "target": "es2020",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "paths": {
      "@google-cloud/monitoring": ["types/monitoring"]
    }
  },
  "include": ["src"],
  "exclude": ["node_modules/"],
}
