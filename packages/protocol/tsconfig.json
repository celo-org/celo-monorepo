{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Recommended",
  "compilerOptions": {
    "target": "es2015",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [
      "node_modules",
      "wagmi.config.ts",
      "abis",
      "scripts/**/*.test.ts"
  ]
}