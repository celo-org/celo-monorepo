{
  "presets": ["next/babel"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "src": "./src",
          "server": "./server"
        }
      }
    ],
    ["react-native-web", { "commonjs": true }],
    [
      "react-css-modules",
      {
        "filetypes": {
          ".scss": {
            "syntax": "postcss-scss"
          }
        },
        "generateScopedName": "[local]___[hash:base64:5]"
      }
    ]
  ]
}
