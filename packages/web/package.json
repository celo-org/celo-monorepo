    "@types/react-autosuggest": "^9.3.11",
    "@types/react-css-modules": "^4.6.1",
    "@types/react-google-recaptcha": "^1.0.0",
    "@types/react-native": "^0.57.41",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-react-css-modules": "^5.0.0",
