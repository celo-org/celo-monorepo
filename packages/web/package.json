    "next-i18next": "^0.36.5",
    "next-images": "^1.1.1",
    "next-plugin-transpile-modules": "^2.0.0",
    "nuka-carousel": "git://github.com/cmcewen/nuka-carousel#fee86bf",
    "react": "^16.8.5",
    "react-autosuggest": "^9.4.3",
    "react-debounce-input": "^3.2.0",
    "react-dom": "^16.8.5",
    "react-google-recaptcha": "^1.1.0",
    "react-is": "^16.8.6",
    "react-lazyload": "^2.5.0",
