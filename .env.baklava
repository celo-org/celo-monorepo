# Genesis Vars
NETWORK_ID=62320
CONSENSUS_TYPE="istanbul"
BLOCK_TIME=5
EPOCH=17280
LOOKBACK=12
ISTANBUL_REQUEST_TIMEOUT_MS=10000

# the number of load test clients that will be given funds in the genesis & migrations
